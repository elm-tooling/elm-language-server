{
  "name": "@elm-tooling/elm-language-server",
  "description": "Implementation of an elm language server in node.",
  "version": "1.3.2",
  "author": "Kolja Lampe",
  "license": "MIT",
  "files": [
    "out"
  ],
  "engines": {
    "node": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elm-tooling/elm-language-server"
  },
  "dependencies": {
    "elm-analyse": "github:elm-tooling/elm-analyse#f2a983fc7bab262a3e44b46732735d8510d18876",
    "execa": "^2.0.4",
    "fast-diff": "^1.2.0",
    "globby": "^10.0.1",
    "request": "^2.88.0",
    "rxjs": "^6.5.2",
    "vscode-languageserver": "^5.2.1",
    "vscode-uri": "^2.0.3",
    "web-tree-sitter": "^0.15.9",
    "ws": "^7.1.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.17",
    "@types/node": "12.7.2",
    "@types/request": "^2.48.2",
    "@types/ws": "^6.0.1",
    "copyfiles": "^2.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "tree-sitter-cli": "^0.15.7",
    "tree-sitter-elm": "^2.5.6",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-sonarts": "^1.9.0",
    "typescript": "3.5.3"
  },
  "scripts": {
    "copy-wasm": "copyfiles ./tree-sitter-elm.wasm out",
    "compile": "npm run copy-wasm && tsc -p ./",
    "watch": "npm run copy-wasm && tsc -watch -p ./",
    "lint": "tslint -p tsconfig.json",
    "format:fix": "pretty-quick --staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "build-tree-sitter": "tree-sitter build-wasm ./node_modules/tree-sitter-elm",
    "test": "jest"
  },
  "bin": {
    "elm-language-server": "./out/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "elm",
    "elm-lang",
    "language server",
    "language-server",
    "lsp"
  ]
}
